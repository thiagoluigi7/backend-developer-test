{
  "name": "backend-developer-test",
  "version": "1.0.0",
  "repository": "https://github.com/thiagoluigi7/backend-developer-test.git",
  "author": "Thiago Luigi <thiagoluigi7@hotmail.com>",
  "license": "MIT",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "rimraf dist .esbuild .serverless dist && cross-env NODE_ENV=development sls offline start --stage offline",
    "dev:local": "tsx watch src/app.ts",
    "dev:test": "jest --watch",
    "deploy": "rimraf dist .esbuild .serverless dist && cross-env NODE_ENV=production sls deploy --stage prod --verbose --force",
    "remove-prod": "cross-env NODE_ENV=production sls remove --stage prod --verbose",
    "tail-log": "sls logs -f app -t",
    "clean": "rimraf dist .esbuild .serverless dist",
    "lint": "eslint .",
    "format": "eslint --fix . && prettier '**/*.{json,yaml}' --write --ignore-path .gitignore",
    "test": "jest --passWithNoTests",
    "prepare": "husky install",
    "postinstall": "prisma generate"
  },
  "engines": {
    "node": ">=20.11.0"
  },
  "dependencies": {
    "@aws-sdk/client-lambda": "^3.513.0",
    "@aws-sdk/client-s3": "^3.513.0",
    "@aws-sdk/client-sqs": "^3.514.0",
    "@prisma/client": "^5.6.0",
    "dotenv": "^16.4.3",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "helmet": "^7.1.0",
    "openai": "^4.28.0",
    "serverless-http": "^3.2.0"
  },
  "devDependencies": {
    "@swc/core": "^1.3.100",
    "@types/aws-lambda": "^8.10.133",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.10",
    "@types/node": "^20.11.17",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "cross-env": "^7.0.3",
    "esbuild": "^0.16.17",
    "eslint": "^8.54.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-jest-formatting": "^3.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "prisma": "^5.6.0",
    "rimraf": "^5.0.5",
    "serverless": "^3.38.0",
    "serverless-dotenv-plugin": "^6.0.0",
    "serverless-esbuild": "^1.51.0",
    "serverless-lift": "^1.28.1",
    "serverless-offline": "^13.3.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.6.1",
    "typescript": "^5.3.2"
  }
}
